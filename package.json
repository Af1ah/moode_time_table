{
  "name": "time_table",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:stop": "lsof -ti:3000 | xargs kill -9 2>/dev/null || echo 'No process on port 3000'",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "clean": "rm -rf .next node_modules .cache package-lock.json && echo 'Cleaned: .next, node_modules, .cache, package-lock.json'",
    "db:migrate:recurring": "psql $DATABASE_URL -f db/recurring_sessions.sql"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "next": "16.0.3",
    "prisma": "5.22.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
